@model Color


<div class="main-panel">
    <h1>Edit Color</h1>
    <form class="mt-3" method="post">
        <div>
            <label>Color:</label><br />
            <input asp-for="ColorName" class="form-control" /><br />
            <span class="text-danger" asp-validation-for="ColorName"></span>
        </div>
        <div class="mt-2">
            <button class="btn btn-success" type="submit">Create</button>
        </div>
    </form>
</div>
